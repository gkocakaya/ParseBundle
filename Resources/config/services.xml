<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="parse_sdk.class">Parse\ParseClient</parameter>
    </parameters>

    <services>
        <service id="parse_sdk.parseclient" class="%parse_sdk.class%" factory-class="%parse_sdk.class%" factory-method="create" lazy="true">
            <argument type="collection">
                <argument key="app_id">%parse_sdk.app_id%</argument>
                <argument key="rest_key">%parse_sdk.rest_key%</argument>
                <argument key="master_key">%parse_sdk.master_key%</argument>
            </argument>
            <argument id="logger" type="service"/>
        </service>
    </services>
</container>